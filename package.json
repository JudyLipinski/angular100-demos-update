{
  "name": "my-angular-demos",
  "version": "0.0.0",
  "config": {
    "serverPort": 3838,
    "angularPort": 4747
  },
  "scripts": {
    "config": "ts-node set-env.ts",
    "start": "run-p json-server startng",
    "startng": "ng serve --port 5556 -o",
    "custom-env": "run-s config start-custom-server-and-angular",
    "start-custom-server-and-angular": "run-p custom-json-server custom-start-ng",
    "custom-json-server": "cross-var json-server --watch people.json --port $npm_package_config_serverPort",
    "custom-start-ng": "cross-var ng serve --port $npm_package_config_angularPort -o",
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "json-server": "json-server --watch people.json --port 5566"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "bootstrap": "^4.5.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "cross-var": "^1.1.0",
    "dotenv": "^8.2.0",
    "json-server": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.51",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
